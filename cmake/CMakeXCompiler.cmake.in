set(CMAKE_X_COMPILER 					"@CMAKE_X_COMPILER@")
set(CMAKE_X_COMPILER_ARG1 				"@CMAKE_X_COMPILER_ARG1@")
set(CMAKE_X_RUNTIME  					"@CMAKE_X_RUNTIME@")
set(CMAKE_X_ARCHIVE  					"@CMAKE_X_ARCHIVE@")
set(CMAKE_LINKER 						"@CMAKE_LINKER@")
set(CMAKE_RANLIB 						"@CMAKE_RANLIB@")
set(CMAKE_X_COMPILER_LOADED 			1)

# Source files extensions for language X
set(CMAKE_X_SOURCE_FILE_EXTENSIONS 		x;X;xpp;XPP)

# Files used for the language but that need to be ignored
set(CMAKE_X_IGNORE_EXTENSIONS 			h;H;o;O;obj;OBJ)

# Value representing the priority of the linker for language 
# X against others languages in the same project
set(CMAKE_X_LINKER_PREFERENCE 			40) # None

# File extension for generated files
set(CMAKE_X_OUTPUT_EXTENSION 			.o)

# Should replace input extension with output extension?
# 0 for: file.x -> file.x.o
# 1 for: file.x -> file.o
set(CMAKE_X_OUTPUT_EXTENSION_REPLACE 	0)

# Suffix for static libraries
set(CMAKE_STATIC_LIBRARY_SUFFIX_X 		".a")

# Prefix for static libraries, ie: libMylib.a or Mylib.a
set(CMAKE_STATIC_LIBRARY_PREFIX_X 		"prefix")

# Env var to use to override the compiler, like CC, CXX
set(CMAKE_X_COMPILER_ENV_VAR 			"XC")
